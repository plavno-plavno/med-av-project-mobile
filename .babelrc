{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src",
          "@api": "./src/api",
          "@assets": "./src/assets",
          "@utils": "./src/utils",
          "@redux": "./src/redux",
          "@features": "./src/features",
          "@navigation": "./src/navigation",
          "@components": "./src/components",
        }
      }
    ],
    "react-native-reanimated/plugin"
  ]
}

